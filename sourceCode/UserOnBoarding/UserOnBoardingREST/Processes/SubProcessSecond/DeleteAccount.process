                                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Input/Msg3"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:otherwise>
                    </xsl:choose>
                </data>
            </ns2:ActivityInput>
        </pd:inputBindings>
