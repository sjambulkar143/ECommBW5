        <pd:x>71</pd:x>
        <pd:y>355</pd:y>
        <config>
            <faultName>FaultSchema</faultName>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <data>
                    <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                        <xsl:when test="string-length($Start/pfx2:DeleteAccountReq/pfx2:CustomerID) = 0">
                            <pfx3:Fault>
                                <pfx3:Message>
