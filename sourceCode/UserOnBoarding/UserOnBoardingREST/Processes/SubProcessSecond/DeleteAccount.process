        <pd:to>Fetch-Account</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>string-length($Start/pfx2:DeleteAccountReq/pfx2:CustomerID) &gt;0 and  uuid:matchRegex($Start/pfx2:DeleteAccountReq/pfx2:CustomerID,"^[a-zA-Z0-9_]*$")</pd:xpath>
