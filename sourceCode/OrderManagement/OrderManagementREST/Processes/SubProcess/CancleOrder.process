<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:pfx="http://www.EniqueSolutions.com/OrderManagement/schemas/CustomFunction2/SharedResources/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/schemas/NewUserOnBoardingSOAP/SharedResources/Schemas/Schema.xsd2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.tibco.com/schemas/OrderManagementsLatestSOAP/SharedResources/Schemas/FaultNewSchema.xsd" xmlns:pfx2="http://www.EniqueSolutions.com/schemas/NewUserOnBoardingSOAP/SharedResources/Schemas/Schema.xsd2" xmlns:uuid="java://Custom.Custom" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xsd:import namespace="http://www.EniqueSolutions.com/OrderManagement/schemas/CustomFunction2/SharedResources/Schemas/Schema.xsd" schemaLocation="/SharedResources/Schemas/OrderManagement.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/OrderManagementsLatestSOAP/SharedResources/Schemas/FaultNewSchema.xsd" schemaLocation="/SharedResources/Schemas/NewFaultSchema.xsd"/>
    <xsd:import namespace="http://www.EniqueSolutions.com/schemas/NewUserOnBoardingSOAP/SharedResources/Schemas/Schema.xsd2" schemaLocation="/SharedResources/Schemas/NewOrderManagement.xsd"/>
    <pd:name>Processes/SubProcess/CancleOrder.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:CancleOrderReq"/>
    <pd:startX>80</pd:startX>
    <pd:startY>35</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="contains($CatchProductID/pfx3:Fault/pfx3:MessageCode,$_globalVariables/ns2:GlobalVariables/NewItem/Msg1) or contains($CatchProductID/pfx3:Fault/pfx3:MessageCode,$_globalVariables/ns2:GlobalVariables/NewItem/Msg2)">
                <pfx2:CancleOrderRes>
                    <pfx2:ResponseMessage>
                        <xsl:value-of select="$_error_ProductDet/pfx3:Fault/pfx3:Message"/>
                    </pfx2:ResponseMessage>
                </pfx2:CancleOrderRes>
            </xsl:when>
            <xsl:when test="contains($CatchProductID/pfx3:Fault/pfx3:MessageCode,$_globalVariables/ns2:GlobalVariables/NewItem/Msg3)">
                <pfx2:CancleOrderRes>
                    <pfx2:ResponseMessage>
                        <xsl:value-of select="$_error_Generate-Error/pfx3:Fault/pfx3:Message"/>
                    </pfx2:ResponseMessage>
                </pfx2:CancleOrderRes>
            </xsl:when>
            <xsl:when test="exists($_error/ns:ErrorReport)">
                <pfx2:CancleOrderRes>
                    <pfx2:ResponseMessage>
                        <xsl:value-of select="&quot;100&quot;"/>
                    </pfx2:ResponseMessage>
                </pfx2:CancleOrderRes>
            </xsl:when>
            <xsl:otherwise>
                <pfx2:CancleOrderRes>
                    <pfx2:ResponseMessage>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/RvNetwork"/>
                    </pfx2:ResponseMessage>
                </pfx2:CancleOrderRes>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:CancleOrderRes"/>
    <pd:endX>562</pd:endX>
    <pd:endY>137</pd:endY>
    <pd:errorSchemas>
        <FaultSchema ref="pfx3:Fault"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1677731605338</pd:targetNamespace>
    <pd:activity name="Product_ID">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>207</pd:x>
        <pd:y>143</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>SELECT PRODUCT_ID
  FROM ES_PRODUCTS WHERE PRODUCT_ID = ?
</statement>
            <jdbcSharedConfig>/SharedResources/connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>PRODUCT_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <QueryOutputCachedSchemaColumns>PRODUCT_ID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <PRODUCT_ID>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Product_ID"/>
                </PRODUCT_ID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="INSERTPRODETAILS">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>320</pd:x>
        <pd:y>140</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>DELETE FROM SYSTEM.ES_ORDERS
 WHERE PRODUCT_ID = ?</statement>
            <jdbcSharedConfig>/SharedResources/connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>PRODUCT_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <PRODUCT_ID>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Product_ID"/>
                </PRODUCT_ID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CatchAllErrors">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>721</pd:x>
        <pd:y>35</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="CatchProductID">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>737</pd:x>
        <pd:y>265</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <fault>localname=Fault namespace=http://www.tibco.com/schemas/OrderManagementsLatestSOAP/SharedResources/Schemas/FaultNewSchema.xsd</fault>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>208</pd:x>
        <pd:y>268</pd:y>
        <config>
            <faultName>FaultSchema</faultName>
        </config>
        <pd:inputBindings>
            <pfx4:ActivityInput>
                <data>
                    <pfx3:Fault>
                        <pfx3:Message>
                            <xsl:value-of select="&quot;ProductID not Exists&quot;"/>
                        </pfx3:Message>
                        <pfx3:MessageCode>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg3"/>
                        </pfx3:MessageCode>
                    </pfx3:Fault>
                </data>
            </pfx4:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="EndLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>460</pd:x>
        <pd:y>140</pd:y>
        <config>
            <processName>/ExceptionProcesses/LogHandlerProcess/LogException.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LogException>
                <ns1:ApplicationName>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns1:ApplicationName>
                <ns1:ProcessName>
                    <xsl:value-of select="&quot;CancelOrderSOAPSub&quot;"/>
                </ns1:ProcessName>
                <ns1:LogType>
                    <xsl:value-of select="&quot;End_Log&quot;"/>
                </ns1:LogType>
                <xsl:if test="$Start/pfx2:CancleOrderReq/pfx2:Correlation_ID">
                    <ns1:CorrelationID>
                        <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Correlation_ID"/>
                    </ns1:CorrelationID>
                </xsl:if>
                <ns1:MessageType>
                    <xsl:value-of select="&quot;EndLogExcecuted&quot;"/>
                </ns1:MessageType>
                <ns1:Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:Timestamp>
            </ns1:LogException>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="StartLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>77</pd:x>
        <pd:y>137</pd:y>
        <config>
            <processName>/ExceptionProcesses/LogHandlerProcess/LogException.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LogException>
                <ns1:ApplicationName>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns1:ApplicationName>
                <ns1:ProcessName>
                    <xsl:value-of select="&quot;CancelOrderSOAPSub&quot;"/>
                </ns1:ProcessName>
                <ns1:LogType>
                    <xsl:value-of select="&quot;Start_Log&quot;"/>
                </ns1:LogType>
                <ns1:CorrelationID>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Correlation_ID"/>
                </ns1:CorrelationID>
                <ns1:MessageType>
                    <xsl:value-of select="&quot;StartLogExcecuted&quot;"/>
                </ns1:MessageType>
                <ns1:Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:Timestamp>
                <ns1:Payload>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Product_ID"/>
                </ns1:Payload>
            </ns1:LogException>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ExceptionHandler">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>563</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/ExceptionProcesses/ExceptionHandlerProcess/ExceptionHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:LogException>
                <ns1:ApplicationName>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns1:ApplicationName>
                <ns1:ProcessName>
                    <xsl:value-of select="&quot;CancelSOAPSub&quot;"/>
                </ns1:ProcessName>
                <ns1:LogType>
                    <xsl:value-of select="&quot;Exception_Log&quot;"/>
                </ns1:LogType>
                <ns1:CorrelationID>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Correlation_ID"/>
                </ns1:CorrelationID>
                <ns1:MessageType>
                    <xsl:value-of select="&quot;ErrorFound&quot;"/>
                </ns1:MessageType>
                <ns1:Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:Timestamp>
                <ns1:Payload>
                    <xsl:value-of select="tib:render-xml($CatchAllErrors/exceptiondata)"/>
                </ns1:Payload>
                <ns1:ExceptionType>
                    <ns1:MessageCode>
                        <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                    </ns1:MessageCode>
                    <ns1:ExceptionMessage>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </ns1:ExceptionMessage>
                    <ns1:StackTrace>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </ns1:StackTrace>
                </ns1:ExceptionType>
            </ns1:LogException>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ExceptionHandlerFile">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>565</pd:x>
        <pd:y>265</pd:y>
        <config>
            <processName>/ExceptionProcesses/ExceptionHandlerProcess/ExceptionHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:LogException>
                <ns1:ApplicationName>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns1:ApplicationName>
                <ns1:ProcessName>
                    <xsl:value-of select="&quot;CancelSOAPSub&quot;"/>
                </ns1:ProcessName>
                <ns1:LogType>
                    <xsl:value-of select="&quot;Exception_Log&quot;"/>
                </ns1:LogType>
                <ns1:CorrelationID>
                    <xsl:value-of select="$Start/pfx2:CancleOrderReq/pfx2:Correlation_ID"/>
                </ns1:CorrelationID>
                <ns1:MessageType>
                    <xsl:value-of select="&quot;ErrorFound&quot;"/>
                </ns1:MessageType>
                <ns1:Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:Timestamp>
                <ns1:Payload>
                    <xsl:value-of select="tib:render-xml($CatchProductID/pfx3:Fault)"/>
                </ns1:Payload>
                <ns1:ExceptionType>
                    <ns1:MessageCode>
                        <xsl:value-of select="$CatchProductID/pfx3:Fault/pfx3:MessageCode"/>
                    </ns1:MessageCode>
                    <ns1:ExceptionMessage>
                        <xsl:value-of select="$CatchProductID/pfx3:Fault/pfx3:Message"/>
                    </ns1:ExceptionMessage>
                    <ns1:StackTrace>
                        <xsl:value-of select="$CatchProductID/pfx3:Fault/pfx3:StackTrace"/>
                    </ns1:StackTrace>
                </ns1:ExceptionType>
            </ns1:LogException>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ProductDet">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>77</pd:x>
        <pd:y>254</pd:y>
        <config>
            <faultName>FaultSchema</faultName>
        </config>
        <pd:inputBindings>
            <pfx4:ActivityInput>
                <data>
                    <xsl:choose>
                        <xsl:when test="not(uuid:matchRegex($Start/pfx2:CancleOrderReq/pfx2:Product_ID, &quot;^[a-zA-Z0-9_]*$&quot;))">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;ProductID must be Alphanumeric Value&quot;"/>
                                </pfx3:Message>
                                <xsl:if test="$_globalVariables/ns2:GlobalVariables/NewItem/Msg1">
                                    <pfx3:MessageCode>
                                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg1"/>
                                    </pfx3:MessageCode>
                                </xsl:if>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:otherwise>
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;ProductID Should not be Empty&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg2"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:otherwise>
                    </xsl:choose>
                </data>
            </pfx4:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>StartLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Product_ID</pd:from>
        <pd:to>INSERTPRODETAILS</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($Product_ID/resultSet/Record/PRODUCT_ID)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>INSERTPRODETAILS</pd:from>
        <pd:to>EndLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Product_ID</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>StartLog</pd:from>
        <pd:to>Product_ID</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>string-length($Start/pfx2:CancleOrderReq/pfx2:Product_ID) &gt;0  and  uuid:matchRegex($Start/pfx2:CancleOrderReq/pfx2:Product_ID, "^[a-zA-Z0-9_]*$")</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>EndLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CatchAllErrors</pd:from>
        <pd:to>ExceptionHandler</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ExceptionHandler</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CatchProductID</pd:from>
        <pd:to>ExceptionHandlerFile</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ExceptionHandlerFile</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>StartLog</pd:from>
        <pd:to>ProductDet</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>