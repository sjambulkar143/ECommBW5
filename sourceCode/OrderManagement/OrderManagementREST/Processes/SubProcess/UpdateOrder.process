    <pd:activity name="CustomerNot-1">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>238</pd:x>
        <pd:y>145</pd:y>
        <config>
            <faultName>FaultSchema</faultName>
        </config>
        <pd:inputBindings>
            <pfx4:ActivityInput>
                <data>
                    <xsl:choose>
                        <xsl:when test="string-length($Start/pfx2:UpdateOrderReq/pfx2:Product_ID) = 0">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;Product_ID should not be Empty&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg1"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:when test="not(uuid:matchRegex($Start/pfx2:UpdateOrderReq/pfx2:Product_ID, &quot;^[a-zA-Z0-9_]*$&quot;))">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;Product_ID must be Alphanumeric Value&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg2"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:when test="not(uuid:matchRegex($Start/pfx2:UpdateOrderReq/pfx2:Order_Date,'^\d{4}[\-\/\s]?((((0[13578])|(1[02]))[\-\/\s]?(([0-2][0-9])|(3[01])))|(((0[469])|(11))[\-\/\s]?(([0-2][0-9])|(30)))|(02[\-\/\s]?[0-2][0-9]))$'))">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;Order_Date must be yyyy-MM-dd format&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg3"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:when test="not(uuid:matchRegex($Start/pfx2:UpdateOrderReq/pfx2:Required_Date,'^\d{4}[\-\/\s]?((((0[13578])|(1[02]))[\-\/\s]?(([0-2][0-9])|(3[01])))|(((0[469])|(11))[\-\/\s]?(([0-2][0-9])|(30)))|(02[\-\/\s]?[0-2][0-9]))$'))">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;Required_Date must be yyyy-MM-dd format&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg4"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:when test="$Start/pfx2:UpdateOrderReq/pfx2:List_Price &lt; $Start/pfx2:UpdateOrderReq/pfx2:Discount">
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;List_Price Always greater than to Discount&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg7"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:when>
                        <xsl:otherwise>
                            <pfx3:Fault>
                                <pfx3:Message>
                                    <xsl:value-of select="&quot;Shipment_Date must be yyyy-MM-dd format&quot;"/>
                                </pfx3:Message>
                                <pfx3:MessageCode>
                                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/NewItem/Msg5"/>
                                </pfx3:MessageCode>
                            </pfx3:Fault>
                        </xsl:otherwise>
                    </xsl:choose>
                </data>
            </pfx4:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
