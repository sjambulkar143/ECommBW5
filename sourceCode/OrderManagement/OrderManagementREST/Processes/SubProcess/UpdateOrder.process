    <pd:activity name="Update_ESOrder">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>272</pd:x>
        <pd:y>249</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>UPDATE ES_ORDERS
  SET QUANTITY = ?,LIST_PRICE = ?,
      DISCOUNT = ?, ORDER_STATUS = ?,ORDER_DATE = TO_DATE(?, 'yyyy-MM-dd'), REQUIRED_DATE = TO_DATE( ?, 'yyyy-MM-dd'), SHIPPED_DATE= TO_DATE( ?, 'yyyy-MM-dd')
  WHERE PRODUCT_ID = ?</statement>
            <jdbcSharedConfig>/SharedResources/connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>QUANTITY</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>LIST_PRICE</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>DISCOUNT</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>ORDER_STATUS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ORDER_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>REQUIRED_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>SHIPPED_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PRODUCT_ID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <QUANTITY>
                    <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Quantity"/>
                </QUANTITY>
                <LIST_PRICE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:List_Price)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:List_Price"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </LIST_PRICE>
                <DISCOUNT>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Discount)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Discount"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </DISCOUNT>
                <ORDER_STATUS>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Order_Status)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Order_Status"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </ORDER_STATUS>
                <ORDER_DATE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Order_Date)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Order_Date"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </ORDER_DATE>
                <REQUIRED_DATE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Required_Date)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Required_Date"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </REQUIRED_DATE>
                <SHIPPED_DATE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Shipped_Date)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Shipped_Date"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </SHIPPED_DATE>
                <PRODUCT_ID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:UpdateOrderReq/pfx2:Product_ID)">
                            <xsl:value-of select="$Start/pfx2:UpdateOrderReq/pfx2:Product_ID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </PRODUCT_ID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
